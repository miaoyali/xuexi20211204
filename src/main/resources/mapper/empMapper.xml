<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<!-- namespace：匹配的接口 -->
<mapper namespace="com.example.demo3.shuju.repository.EmpRepository1">
    <select id="findOne"
            resultType="com.example.demo3.shuju.EmpClass">
        SELECT
        name,age,birth
        FROM
        emp
        WHERE name=#{name}
    </select>

</mapper>